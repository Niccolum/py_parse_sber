{
  "version": 1,
  "disable_existing_loggers": false,

  "formatters": {
    "simple": {
      "format": "%(asctime)s | %(name)s | %(levelname)s | %(message)s"
    }
  },

  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },

  "loggers": {
    "sberbank_parse": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
      "__main__": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
      "utils": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "abstract": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    }
  }
}
